{
  "name": "@tsmt/api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf .turbo dist build *.tsbuildinfo",
    "start": "node dist/cmd/http/main.js",
    "dev": "cross-env tsnd --files --respawn --cls --exit-child src/cmd/http/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "prettier --write .",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:hook": "vitest run --changed HEAD~1",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:resolve": "prisma migrate resolve",
    "prisma:status": "prisma migrate status",
    "seed": "ts-node ./src/seed.ts"
  },
